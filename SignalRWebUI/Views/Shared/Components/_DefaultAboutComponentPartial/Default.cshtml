@model List<ResultAboutDto>
@{
}
<!-- about section -->

<section class="about_section layout_padding">
    <div class="container  ">
        
        <div class="row">
           
            <div class="col-md-6 ">
                <div class="img-box">
                    <img src="~/feane-1.0.0/images/about-img.png" alt="">
                        
                </div>
            </div>
           
            <div class="col-md-6">
                @foreach (var item in Model)
                {
                <div class="detail-box">
                    <div class="heading_container">
                        <h2>
                            @item.Title
                        </h2>
                    </div>
                    <p>
                        @item.Description
                    </p>
                    <a href="" class="m-2">
                        Read More
                    </a>
                </div>
                }
            </div>
          
        </div>
    
        <div>
           
        </div>
    </div>
   
</section>

<!-- end about section -->
